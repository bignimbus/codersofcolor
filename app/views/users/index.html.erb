    <!-- MEMBER -->
   <div class="row">
      <% @users.each do |user| %>

  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <div class="caption">
        <h3><%= user.name %></h3>
        <p><%= user.skill %></p>
        <!-- <%= raw @users.collect { |field| link_to user.field, field }.join(",") %> -->
        <p><%= user.bio %></p>
          <%= link_to 'Website', user.website, class: "btn btn-info" %>
        </p>
      </div>
    </div>
  </div>
</div>
  <% end %>
    <!-- / END MEMBER -->
